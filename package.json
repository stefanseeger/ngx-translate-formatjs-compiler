    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.1",
    "intl-messageformat": "^10.1.0",
    "jest": "^28.1.1",
    "jest-preset-angular": "^12.1.0",
    "lint-staged": "^13.0.1",
    "ng-packagr": "^14.0.0",
    "prettier": "^2.7.0",
    "rxjs": "~7.5.0",
